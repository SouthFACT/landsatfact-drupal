/**
 * Implements hook_google_analytics_et_settings_info().
 */
function lsf_analytics_event_google_analytics_et_settings_info() {
  $settings = array();

  $settings['debug'] = FALSE;

  return $settings;
}

/**
 * Implements hook_google_analytics_et_api().
 */
